{
    "version": 2,
    "routes": [
      {
        "src": "/api/proxy",
        "methods": ["GET", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type"
        }
      }
    ],
    "functions": {
      "api/proxy.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }