{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1",
        "methods": ["GET", "OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET, OPTIONS",
          "Access-Control-Allow-Headers": "Content-Type, User-Agent"
        }
      }
    ],
    "functions": {
      "api/**/*.ts": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }